{
  "name": "snape-node",
  "version": "1.0.0",
  "description": "Snape node - for studio management",
  "main": "index.js",
  "scripts": {
    "build": "tsc --outDir dist",
    "start": "tsc --watch & nodemon --inspect dist",
    "dev": "concurrently \"npx tsc --watch\" \"nodemon -q dist/server.js\"",
    "typeorm": "typeorm-ts-node-commonjs",
    "migration:run": "ts-node ./node_modules/typeorm/cli.js  -d dist/db/db.config.js migration:run",
    "migration:revert": "ts-node ./node_modules/typeorm/cli.js  -d dist/db/db.config.js migration:revert",
    "schema:sync": "npm run build && npm run typeorm schema:sync -- -d dist/db/db.config.js",
    "migration:show": "npm run typeorm migration:show -- -d dist/db/db.config.js",
    "migration:generate": "npm run build && npm run typeorm migration:generate -- --dataSource dist/db/db.config.js src/migration/latestmigration ",
    "migration:create": "npm run typeorm migration:create"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CodeStar2244/Snape-node.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/CodeStar2244/Snape-node/issues"
  },
  "homepage": "https://github.com/CodeStar2244/Snape-node#readme",
  "dependencies": {
    "body-parser": "^1.20.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "i18next": "^22.4.11",
    "i18next-fs-backend": "^2.1.1",
    "i18next-http-middleware": "^3.3.0",
    "jm-ez-l10n": "^1.0.0",
    "jsonwebtoken": "^9.0.0",
    "lodash": "^4.17.21",
    "moment-timezone": "^0.5.41",
    "morgan": "^1.10.0",
    "pg": "^8.10.0",
    "ts-node": "^10.9.1",
    "typeorm": "^0.3.12",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "typescript": "^4.9.5"
  }
}
